package com.xworkz.finalkeyword;

public class Specs {
	
	public Specs()
	{
		System.out.println("non-parameterized specs const");
	}
	public void wear()
	{
		System.out.println("running wear method in specs");
	}
    public static void damage()
    {
    	System.out.println("running damage");
    }
    public static void clean()
    {
    	System.out.println("running clean");
    }

}
