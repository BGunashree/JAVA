package com.xworkz.array;

public class Diagonal {

	public static void main(String[] args) {


		
		int[][] a= {{1,2,3},{4,5,6},{7,8,9}};
		int  row=3;
		int col=3;
		int sum=0;
		int diagonalsum=0;
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<col;j++)
			{
				sum=sum+a[i][j]*a[i][j];
			}
			System.out.println();
		}

	}

}
